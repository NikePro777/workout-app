{
	"trailingComma": "none",
	"tabWidth": 2,
	"useTabs": true,
	"semi": false,
	"singleQuote": false,
	"jsxSingleQuote": true,
	"arrowParens": "avoid",
    "importOrder": [
        "<THIRD_PARTY_MODULES>",
        "/routes/",
        "/middleware/",
        "^../(.*)",
        "^./(.*)"
    ],
"importOrderSeparation": true,
"importOrderSortSpecifiers": true
}
